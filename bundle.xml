<?xml version="1.0" encoding="utf-8"?>
<adapt_framework>
  <bundle>
    <name>bootstrap</name>
    <version>3.3.2</version>
    <version_status>release</version_status>
    <availability>public</availability>
    <type>css_framework</type>
    <namespace>\bootstrap</namespace>
    <label>Bootstrap</label>
    <description>Bootstrap is the most popular HTML, CSS and JS framework for developing responsive, mobile first projects on the web.</description>
    <copyright>2011 - 2016 Twitter</copyright>
    <license>MIT</license>
    <vendor>
        <name>Adapt Framework</name>
        <website>https://www.adaptframework.com</website>
    </vendor>
    <settings>
      <category name="Mobile display">
        <setting>
          <name>bootstrap.allow_zooming</name>
          <label>Should mobile devices be allowed to zoom?</label>
          <allowed_values>
            <value>Yes</value>
            <value>No</value>
          </allowed_values>
          <default_values>
            <value>No</value>
          </default_values>
        </setting>
      </category>
    </settings>
    <depends_on>
      <bundle>
        <name>jquery</name>
      </bundle>
      <bundle>
        <name>adapt</name>
        <version>2.0</version>
      </bundle>
    </depends_on>
  </bundle>
</adapt_framework>
